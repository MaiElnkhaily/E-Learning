@model IEnumerable<ELearningManagementSystem.Models.Result>
@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}
<div id="page-wrapper" onLoad="noBack();" onpageshow="if (event.persisted) noBack();" onUnload="">
    <!--start lec-->
    <div class="container-fluid">
        <!-- Page Heading -->
        <!-- Content Panel -->
        <div class="row" id="main">

            <div class="col-md-9 personal-info">

                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Student ID:</label>
                        <div class="col-lg-8">
                            <input class="form-control" type="text" value="@ViewBag.id" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Student name:</label>
                        <div class="col-lg-8">
                            <input class="form-control" type="text" value="@ViewBag.pn" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Department:</label>
                        <div class="col-lg-8">
                            <input class="form-control" type="text" value="@ViewBag.dep" disabled>
                        </div>
                    </div>
                </form>
            </div>


            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th scope="col" id="name">Course name</th>
                        <th scope="col" id="code">Question Type</th>
                        <th scope="col" id="exam">Exercises</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>

                            <td> @ViewBag.c_name</td>
                            <td> @Html.DisplayFor(ww => item.QType)</td>
                            <td> @Html.DisplayFor(ww => item.Exercises)</td>

                        </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>
</div>
@section scris{

    <script type="text/javascript">
        window.history.forward();
        function noBack() {
            window.history.forward();
        }
    </script>

}
<!--end loading-->
