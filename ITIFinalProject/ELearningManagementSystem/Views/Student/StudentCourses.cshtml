@model IEnumerable<ELearningManagementSystem.Models.Cours>

@{
    ViewBag.Title = "StudentCourses";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}



<div id="page-wrapper" onLoad="noBack();" onpageshow="if (event.persisted) noBack();" onUnload="">
    <div class="container-fluid courses">
        <!-- Page Heading -->
        <div class="row" id="main">
            <div class="col-sm-12 col-md-12 well" id="content">

                @foreach (var item in Model)
                {
                    <div class="col-lg-4">
                        <a href="/Student/CourseLectures/@item.Course_Code"> <img src="~/images/@item.Photo" class="img-rectangle"> </a>

                    </div>
                }

            </div>
        </div>
    </div>
</div>

@section scris{

    <script type="text/javascript">
        window.history.forward();
        function noBack() {
            window.history.forward();
        }
    </script>

}
