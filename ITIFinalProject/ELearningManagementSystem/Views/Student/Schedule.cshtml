@{
    ViewBag.Title = "Schedule";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<style>
    body {
        background-color: #eee;
    }
</style>


<div style="height:700px;width:900px;margin:0 auto" onLoad="noBack();" onpageshow="if (event.persisted) noBack();" onUnload="">
    @Html.Raw(Model.Render())
</div>
@section scris{

    <script type="text/javascript">
        window.history.forward();
        function noBack() {
            window.history.forward();
        }
    </script>

}