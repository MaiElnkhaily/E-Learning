@model IEnumerable<ELearningManagementSystem.Models.News>
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <!--IE comptability Meta-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--First Mobile Meta-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <link href="~/Resources/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Resources/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/media.css" rel="stylesheet" />
    <link href="~/Content/default_theme.css" rel="stylesheet" />
    <link href="~/Content/hover.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/home.css" rel="stylesheet" />

    <link rel='http://fonts.googleapis.com/css?family=Droid+Sans:400,700'>
    <!--[if it IE 9]-->
    <script src="~/Scripts/html5shiv.min.js"></script>
    <script src="~/Scripts/respond.min.js"></script>
    <!--[end is]-->
</head>

<body onLoad="noBack();" onpageshow="if (event.persisted) noBack();" onUnload="">


    <!--start navbar-->
    <nav class="navbar navbar-default  navbar-fixed-top">
        <div class="container">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#ournavbar"
                        aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <div class=logopi> <img class="logo" width="50px" height="50px" src="~/images/logo.jpg"> </div>
                <!--<span >ITI</span>-->
                <!---- </a> -->
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="ournavbar">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="/home/home">
                            <i class="fa fa-home"></i> HOME <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li><a href="#about"><i class="fa fa-info"></i> ABOUT US </a></li>

                    <li><a href="#new"><i class="fa fa-newspaper-o"></i> NEWS </a></li>
                    <li><a href="#con"><i class="fa fa-phone"></i> CONTACT </a></li>
                    <li><a href="#" id="contact"><i class="fa fa-user"></i> LOGIN</a></li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
    <!--end navbar-->
    <!--start login-->
    <div id="contactForm">
        <span class="login100-form-logo">
            <img src="~/images/logo.jpg" height="150px" width="150px" style="padding-bottom:0px;margin-bottom:5px">
        </span>
        <form class="login" action="/home/login" method="post">
            <h1>LOGIN</h1>
            <div class="alert alert-danger" style="display:none" id="err">@ViewBag.error</div>
            <input type="text" placeholder="Username" name="UserName" required class="u">

            <input type="password" placeholder="Password" name="Password" required class="p">


            <div class="selector">
                <select required name="Type">
                    <option selected value="Professor">Professor</option>
                    <option value="Student">Student</option>
                    <option value="Admin">Admin</option>
                </select>
            </div>

            <button class="btn btn-primary" type="submit"><i class="fa fa-user"></i> Login</button>
            <button class="btn btn-danger"><i class="fa fa-times"></i> Cancel</button>
        </form>
    </div>
    <!--end login-->
    <!--start carousal-->
    <div id="myslide" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators hidden-xs">
            <li data-target="#myslide" data-slide-to="0" class="active"></li>
            <li data-target="#myslide" data-slide-to="1"></li>
            <li data-target="#myslide" data-slide-to="2"></li>

        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="~/images/3.jpg" alt="img1" class="about text-center wow fadeIn" data-wow-duration="5s"
                     data-wow-delay=".1s" data-wow-offset="200" data-wow-iteration="1">
                <div class="carousel-caption hidden-xs">
                    <h3> E-Learning Management System </h3>
                    <h1>Best Online Learning System</h1>
                </div>
            </div>
            <div class="item">
                <img src="~/images/1.jpeg" alt="img2" class="about text-center wow fadeIn" data-wow-duration="5s"
                     data-wow-delay=".1s" data-wow-offset="200" data-wow-iteration="1">
                <div class="carousel-caption hidden-xs">
                    <h3>E-Learning Management System</h3>
                    <h1>Online Free Course</h1>
                </div>
            </div>
            <div class="item">
                <img src="~/images/2.jpg" alt="img3" class="about text-center wow fadeIn" data-wow-duration="5s"
                     data-wow-delay=".1s" data-wow-offset="200" data-wow-iteration="1">
                <div class="carousel-caption hidden-xs">
                    <h3>E-Learning Management System</h3>
                    <h1>Education is a Key to Success</h1>
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myslide" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myslide" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <!--end carousal-->
    <!--start about-->
    <div id="about">
        <section class="about-us text-center">
            <div class="data">
                <div class="container">
                    <h1 class="about text-center wow pulse" data-wow-duration=".5s" data-wow-delay=".5s" data-wow-iteration="2">
                        KNOW MORE ABOUT US
                    </h1>
                    <div id="text-carousel" class="carousel slide" data-ride="carousel">
                        <!-- Wrapper for slides -->
                        <div class="row">
                            <div class="col-xs-offset-3 col-xs-6">
                                <div class="carousel-inner">
                                    <div class="item active">
                                        <div class="carousel-content">
                                            <div>
                                                <Span class="s">The first goal</Span>
                                                <p class="p">
                                                    The aim of our project is to facilitate the Learning process on the students and professor who lives a broad, Where our motto is Study efficiently during
                                                    COVID-19 pandemic.

                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="item">
                                        <div class="carousel-content">
                                            <div>
                                                <Span class="s">The second goal</Span>
                                                <p class="p">
                                                    Participate in organizing and active in the development of the national and global
                                                    knowledge base by initiating systematic scientific research and technical development in
                                                    accordance with the needs of the society and the requirements of its economic, social and
                                                    cultural development, and contributes to increasing its competitiveness.
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="item">
                                        <div class="carousel-content">
                                            <div>
                                                <Span class="s">The third goal</Span>
                                                <p class="p">
                                                    To invest the science and technology in the administration of the university and to
                                                    develop the relations of cooperation and democratic interaction among its elements and to
                                                    integrate the concepts and mechanisms of continuous development and the overall quality in its
                                                    organizational structure and work systems, which will serve as a model for other organizations
                                                    and sectors of society.
                                                </p>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- Controls -->
                        <a class="left carousel-control" href="#text-carousel" data-slide="prev">
                            <span class="glyphicon glyphicon-chevron-left"></span>
                        </a>
                        <a class="right carousel-control" href="#text-carousel" data-slide="next">
                            <span class="glyphicon glyphicon-chevron-right"></span>
                        </a>

                    </div>
                </div>
                <div class="row about text-center wow slideInDown" data-wow-duration="2s" data-wow-delay=".1s"
                     data-wow-offset="200" data-wow-iteration="1">
                    <div class="col-md-4 col-xs-4">
                        <div class="stats">
                            <i class="fa fa-graduation-cap fa-5x"> </i>
                            <p>@TempData["stds"].ToString()</p>
                            <span>Students</span>
                        </div>
                    </div>
                    <div class="col-md-4 col-xs-4">
                        <div class="stats">
                            <i class="fa fa-fw fa-users fa-5x"> </i>
                            <p>@TempData["profs"].ToString()</p>
                            <span>Professors</span>
                        </div>
                    </div>
                    <div class="col-md-4 col-xs-4">
                        <div class="stats">
                            <i class="fa fa-fw fa-book fa-5x"> </i>
                            <p>@TempData["count"].ToString()</p>
                            <span>Courses</span>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!--end about-->
    <!--start news-->
    <section class="news" id="new">
        <div class="container">
            <div class="row">
                <h1>LATEST NEWS</h1>
                @foreach (var item in Model)
                {

                    <div class="col-sm-6 col-md-4 wow slideInDown" data-wow-duration="2s" data-wow-delay=".2s" data-wow-offset="200"
                         data-wow-iteration="1">
                        <div class="thumbnail">
                            <img src="~/images/01.jpg" alt="...">
                            <div class="caption">
                                <h3>@item.Adress</h3>
                                <p>@item.Text</p>
                               @* <p><a href="#" class="btn btn-default" role="button">Read more...</a></p>*@
                            </div>
                        </div>
                    </div>
                }
                @*<div class="col-sm-6 col-md-4 wow slideInDown" data-wow-duration="3s" data-wow-delay=".3s" data-wow-offset="200"
                         data-wow-iteration="1">
                        <div class="thumbnail">
                            <img src="~/images/04.jpg" alt="...">
                            <div class="caption">
                                <h3>Thumbnail label</h3>
                                <p>...</p>
                                <p><a href="#" class="btn btn-default" role="button">Read more...</a></p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-4 wow slideInDown" data-wow-duration="4s" data-wow-delay=".4s" data-wow-offset="200"
                         data-wow-iteration="1">
                        <div class="thumbnail">
                            <img src="~/images/03.jpg" alt="...">
                            <div class="caption">
                                <h3>Thumbnail label</h3>
                                <p>...</p>
                                <p><a href="#" class="btn btn-default" role="button">Read more...</a></p>
                            </div>
                        </div>
                    </div>*@
            </div>
        </div>
    </section>
    <!--end news-->
    <!--start contact us-->

    <div id="con">
        <section class="contact-us text-center">
            <div class="fields">
                <div class="container">
                    <div class="row">
                        <i class="fa fa-headphones fa-5x"></i>
                        <h1>contact</h1>
                        <p class="lead"> To Contact us any time visit </p>
                        <span class="lead">
                            Please Click Here

                        </span>
                        <a href="https://www.facebook.com/ITISuezCanalBranch/"> ITI SuezCanalBranch</a>


                    </div>
                </div>
            </div>
        </section>
    </div>
    <!--end contact us-->
